@model SHOPTHL.Data.Nhacungcap

@{
    ViewData["Title"] = "Delete";
    Layout = "_Admin";
}

<h1>Xác nhận có muốn xóa ?</h1>


<div>
 
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Tên nhà cung cấp:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Tenncc)
        </dd>
        <dt class = "col-sm-2">
            Số điện thoại:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Sdt)
        </dd>
        <dt class = "col-sm-2">
            Địa chỉ:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Diachi)
        </dd>
        <dt class = "col-sm-2">
            Email:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Mancc" />
        <input type="submit" value="Xóa" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-info">Trở về trang danh sách</a>
    </form>
</div>
